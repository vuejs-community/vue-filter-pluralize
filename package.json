{
  "name": "vue-filter-pluralize",
  "description": "Simple pluralize filter for Vue.js",
  "keywords": [
    "vue",
    "vue-filter"
  ],
  "license": "MIT",
  "version": "1.0.0",
  "author": "Eduard Nikolenko <info@eduardnikolenko.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduardnikolenko/vue-filter-pluralize.git"
  },
  "files": [
    "dist/vue-filter-pluralize.cjs.js",
    "dist/vue-filter-pluralize.esm.js"
  ],
  "main": "dist/vue-filter-pluralize.cjs.js",
  "module": "dist/vue-filter-pluralize.esm.js",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "rollup --config rollup.config.js --format cjs --file dist/vue-filter-pluralize.cjs.js",
    "build:esm": "rollup --config rollup.config.js --format esm --file dist/vue-filter-pluralize.esm.js",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "devDependencies": {
    "rollup": "^0.67.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-typescript": "^1.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "vue": "^2.5.17"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  }
}
